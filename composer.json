{
	"name": "lsr/request",
	"description": "Laser framework core - Requests.",
	"version": "0.2.0",
	"repositories": [
		{
			"type": "composer",
			"url": "https://packages.laserliga.cz"
		}
	],
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"phpunit/phpunit": "^9.5",
		"phpstan/phpstan": "^1.8"
	},
	"autoload": {
		"psr-4": {
			"Lsr\\Core\\Requests\\": "src/"
		}
	},
	"authors": [
		{
			"name": "Tomáš Vojík",
			"email": "vojik@wboy.cz"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": ">=8.1",
		"lsr/helpers": "^0.2-dev",
		"lsr/routing": "^0.2-dev",
		"lsr/interfaces": "^0.2-dev",
		"psr/http-message": "1.0.x-dev"
	},
	"extra": {
		"branch-alias": {
			"v0.2": "0.2.x-dev"
		}
	}
